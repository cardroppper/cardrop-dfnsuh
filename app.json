
{
  "expo": {
    "name": "CarDrop",
    "slug": "cardrop",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/d8bffe89-911e-4284-81e6-c7ce7f30e2f1.png",
    "scheme": "cardrop",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/d8bffe89-911e-4284-81e6-c7ce7f30e2f1.png",
      "resizeMode": "contain",
      "backgroundColor": "#1a1a1a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.cardrop.app",
      "icon": "./assets/images/d8bffe89-911e-4284-81e6-c7ce7f30e2f1.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/d8bffe89-911e-4284-81e6-c7ce7f30e2f1.png",
        "backgroundColor": "#1a1a1a"
      },
      "package": "com.cardrop.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ]
  }
}
